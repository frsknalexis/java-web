<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<navigation-rule>
    <from-view-id>/login.xhtml</from-view-id>
     <navigation-case>
     <from-outcome>error</from-outcome>
        <to-view-id>/xhtml/error.xhtml</to-view-id>
        <redirect/>
    </navigation-case>
    <navigation-case>
        <from-outcome>principal</from-outcome>
        <to-view-id>/xhtml/principal.xhtml</to-view-id>
        <redirect/>
    </navigation-case>
     <navigation-case>
   <from-outcome>LISTAR_USUARIOS</from-outcome>
    <to-view-id>/xhtml/ListarUsuarios.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
    <from-outcome>LISTAR_CATEGORIAS</from-outcome>
    <to-view-id>/xhtml/ListarCategorias.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
    <from-outcome>LISTAR_PROVEEDORES</from-outcome>
    <to-view-id>/xhtml/ListarProveedores.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
    <from-outcome>LISTAR_PRODUCTOS</from-outcome>
    <to-view-id>/xhtml/ListarProductos.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
    <from-outcome>LISTAR_CLIENTES</from-outcome>
    <to-view-id>/xhtml/ListarClientes.xhtml</to-view-id>
    </navigation-case>
</navigation-rule>
	
</faces-config>
